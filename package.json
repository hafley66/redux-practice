{
  "name": "redux-practice",
  "version": "1.0.0",
  "description": "Practicing with redux for apps that aren't view centric",
  "main": "index.js",
  "scripts": {
    "test": "npm run mocha:babel:watch",
    "mocha": "echo $(pwd); ./scripts/test.sh",
    "mocha:watch": "npm run mocha -- --watch",
    "mocha:babel": "TEST_FOLDER=src npm run mocha -- --compilers js:babel-core/register",
    "mocha:babel:watch": "TEST_FOLDER=src npm run mocha:babel -- --watch",
    "babel": "babel lib --out-dir tmp",
    "babel:mocha": "npm run babel && npm run mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hafley66/redux-practice.git"
  },
  "author": "Chris Hafley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hafley66/redux-practice/issues"
  },
  "homepage": "https://github.com/hafley66/redux-practice#readme",
  "devDependencies": {
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "redux-cli-logger": "^2.0.0",
    "rimraf": "^2.6.2",
    "sinon": "^4.0.1"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "g": "^2.0.1",
    "js-yaml": "^3.10.0",
    "lodash": "^4.17.4",
    "react": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-subscribe": "^1.0.2",
    "redux-thunk": "^2.2.0",
    "webpack": "^2",
    "webpack-manifest-plugin": "^1.3.2"
  }
}
